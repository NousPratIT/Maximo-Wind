      <numericvalueinfo value="99" description="99" />
    </specify_numeric_domain>

    <specify_aln_domain maxtype="ALN" length="1" domainid="REFSPECIALCHAR" description="HTML/XML Special characters &amp;&quot;'()%&lt;&gt;" overwrite="false">
      <alnvalueinfo value="&amp;" description="&amp;" />
      <alnvalueinfo value="&quot;" description="&quot;" />
      <alnvalueinfo value="'" description="'" />
      <alnvalueinfo value="(" description="(" />
      <alnvalueinfo value=")" description=")" />
      <alnvalueinfo value="%" description="%" />
      <alnvalueinfo value="&lt;" description="&lt;" />
      <alnvalueinfo value="&gt;" description="&gt;" />
    </specify_aln_domain>

    <specify_aln_domain maxtype="ALN" length="256" domainid="REFSCRIPTINJECT" description="Script injection" overwrite="false">
      <alnvalueinfo value="&lt;script&gt;alert('domainvalue');&lt;/script&gt;&lt;img src=" description="&gt;&lt;script&gt;alert('domainvalue');&lt;/script&gt;" />
      <!--alnvalueinfo value="&gt;&lt;script&gt;alert('domainvalue');&lt;/script&gt;&lt;img src=" description="&gt;&lt;script&gt;alert('domainvalue');&lt;/script&gt;&lt;img src=" /-->
      <!--alnvalueinfo value="&quot;&gt;&lt;script&gt;alert('quoteddomainvalue');&lt;/script&gt;&lt;img src=&quot;" description="&quot;&gt;&lt;script&gt;alert('quoteddomainvalue');&lt;/script&gt;&lt;img src=&quot;" / -->
    </specify_aln_domain>

    <specify_aln_domain maxtype="UPPER" length="3" domainid="REFMATCH" description="String matching" overwrite="false">
      <alnvalueinfo value="1X1" description="Description - 3 - Key(Middle)" />
      <alnvalueinfo value="X2" description="X Description - 2 - Key &amp; Desc(Start)" />
      <alnvalueinfo value="X1" description="Description - 1 - Key(Start)" />
      <alnvalueinfo value="Z2" description=" X Description - 6 - Desc(Middle)" />
      <alnvalueinfo value="Z1" description="X Description - 5 - Desc(Start)" />
      <alnvalueinfo value="1X2" description=" X Description - 4 - Key &amp; Desc(Middle)" />
    </specify_aln_domain>

    <specify_aln_domain maxtype="ALN" length="10" domainid="REFENTITY" description="HTML/XML Entity refs: &amp;amp;&amp;quot;&amp;#39;&amp;#40;&amp;#41;&amp;#37;&amp;lt;&amp;gt;" overwrite="false">
