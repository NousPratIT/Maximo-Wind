      <alnvalueinfo value="B" description="Letter B" />
    </specify_aln_domain>
    <specify_aln_domain maxtype="UPPER" length="1" domainid="REFJK" description="Simple domain with two items: J and K" overwrite="false">
      <alnvalueinfo value="J" description="Letter J" />
      <alnvalueinfo value="K" description="Letter K" />
    </specify_aln_domain>
    <specify_aln_domain maxtype="UPPER" length="1" domainid="REFXY" description="Simple domain with two items: X and Y" overwrite="false">
      <alnvalueinfo value="X" description="Letter X" />
      <alnvalueinfo value="Y" description="Letter Y" />
    </specify_aln_domain>

