      <alnvalueinfo value="&amp;#41;" description="&amp;#41;" />
      <alnvalueinfo value="&amp;#37;" description="&amp;#37;" />
      <alnvalueinfo value="&amp;lt;" description="&amp;lt;" />
      <alnvalueinfo value="&amp;gt;" description="&amp;gt;" />
    </specify_aln_domain>


    <define_table persistent="true" type="system" object="REFTABLE4" description="Table for a table domain with 4 values" service="CUSTAPP" classname="psdi.app.refapp.RefTable4ValuesSet" primarykey="VALUE" mainobject="false" internal="false">
      <attrdef attribute="VALUE"   maxtype="SMALLINT" remarks="-" title="Small Int Key" />
      <attrdef attribute="AB"    maxtype="UPPER"    remarks="-" title="Upper (1 char) Domain AB" domain="REFAB" length="1"/>
      <attrdef attribute="DESCRIPTION" maxtype="ALN" remarks="-" title="Alphnumeric (10 char) number word ie 'One'..." length="10" />
    </define_table>
    <freeform description="Initialize 4 values in REFTABLE4">
      <sql target="all">
		insert into REFTABLE4 (VALUE, AB, DESCRIPTION) values (1, 'A', 'One');
		insert into REFTABLE4 (VALUE, AB, DESCRIPTION) values (2, 'A', 'Two');
		insert into REFTABLE4 (VALUE, AB, DESCRIPTION) values (3, 'B', 'Three');
		insert into REFTABLE4 (VALUE, AB, DESCRIPTION) values (4, 'B', 'Four');
	  </sql>
    </freeform>
	<specify_table_domain domainid="REFTABLE4" objectname="REFTABLE4" validationwhereclause="1=1" description="Table domain with 4 values" listwhereclause="1=1" />

    <define_table persistent="true" type="system" object="REFTABLEHEX" description="Table for a table domain with Hex values" service="CUSTAPP" classname="psdi.app.refapp.RefTable4ValuesSet" primarykey="VALUE" mainobject="false" internal="false">
      <attrdef attribute="VALUE"   maxtype="SMALLINT" remarks="-" title="Small Int Key" />
      <attrdef attribute="HEXVAL"    maxtype="ALN"    remarks="-" title="Hex Character for value" domain="REFHEX" length="3"/>
      <attrdef attribute="DESCRIPTION" maxtype="ALN" remarks="-" title="Alphnumeric number word ie 'Ten'..." length="10" />
    </define_table>
    <freeform description="Initialize 6 values in REFTABLEHEX">
      <sql target="all">
		insert into REFTABLEHEX (VALUE, HEXVAL, DESCRIPTION) values (10, '0xA', 'Ten');
		insert into REFTABLEHEX (VALUE, HEXVAL, DESCRIPTION) values (11, '0xB', 'Eleven');
		insert into REFTABLEHEX (VALUE, HEXVAL, DESCRIPTION) values (12, '0xC', 'Twelve');
		insert into REFTABLEHEX (VALUE, HEXVAL, DESCRIPTION) values (13, '0xD', 'Thirteen');
		insert into REFTABLEHEX (VALUE, HEXVAL, DESCRIPTION) values (14, '0xE', 'Fourteen');
		insert into REFTABLEHEX (VALUE, HEXVAL, DESCRIPTION) values (15, '0xF', 'Fifteen');
	  </sql>
    </freeform>
	<specify_table_domain domainid="REFTABLEHEX" objectname="REFTABLEHEX" validationwhereclause="1=1" description="Table domain with hex values" listwhereclause="1=1" />

    <define_table persistent="true" type="system" object="REFTABLEEMPTY" description="Table for an empty table domain" service="CUSTAPP" classname="psdi.app.refapp.RefTableEmptySet" primarykey="REFTABLEEMPTYID" mainobject="false" internal="false">
      <attrdef attribute="KEY"   maxtype="SMALLINT" remarks="-" title="Small Int Key" />
      <attrdef attribute="VALUE" maxtype="ALN"      remarks="-" title="Alphanumeric (20 char)" length="20"  />
    </define_table>
	<specify_table_domain domainid="REFTABLEEMPTY" objectname="REFTABLEEMPTY" validationwhereclause="1=1" description="Table domain with no values" listwhereclause="1=1" />

    <create_app app="REFBASIC" description="Reference Application" maintbname="REFMAIN" />
    <create_app app="REFMOBILE" description="Mobile" maintbname="REFMAIN" />
    <create_app app="REFNEG" description="Negative Tests" maintbname="REFMAIN" />
    <create_app app="REFCOMM" description="Server Communications" maintbname="REFMAIN" />

    <define_table persistent="true" type="system" object="REFMAIN" description="Main MBO for Reference App Basic" service="CUSTAPP" classname="psdi.app.refapp.RefMainSet" primarykey="REFMAINID" mainobject="false" internal="false">
      <attrdef attribute="KEY"         maxtype="UPPER" defaultvalue="&amp;AUTOKEY&amp;" title="Key" remarks="Key Attribute with AutoKey" autokey="REF_KEY" length="10" required="true" mustbe="false" />
      <attrdef attribute="CONTROL"     maxtype="ALN"   remarks="-" title="Control(s) under test" length="30" required="true" />
      <attrdef attribute="DESCRIPTION" maxtype="ALN"   remarks="-" title="Description of test data" length="100" required="true" />

      <attrdef attribute="REF_ALN10"         maxtype="ALN"      remarks="-" title="Alphanumeric (10)" length="10" />
      <attrdef attribute="REF_ALN10_REQ"     maxtype="ALN"      remarks="-" title="Alphanumeric (10) Required" length="10" required="true" defaultvalue="defaultval" />
      <attrdef attribute="REF_ALN100_WITHLD" maxtype="ALN"      remarks="-" title="Alphanumeric (100) with long description" length="100" haslongdesc="true" localizable="true" />
      <attrdef attribute="REF_ALN900"        maxtype="ALN"      remarks="-" title="Alphanumeric (900)" length="900"  />
      <attrdef attribute="REF_UPPER10"       maxtype="UPPER"    remarks="-" title="Upper (10)" length="10" />
      <attrdef attribute="REF_LOWER10"       maxtype="LOWER"    remarks="-" title="Lower (10)" length="10" />
      <attrdef attribute="REF_YORN"          maxtype="YORN"     remarks="-" title="YorN" />
      <attrdef attribute="REF_DATE"          maxtype="DATE"     remarks="-" title="Date" />
      <attrdef attribute="REF_DATETIME"      maxtype="DATETIME" remarks="-" title="DateTime" />
      <attrdef attribute="REF_TIME"          maxtype="TIME"     remarks="-" title="Time" />
      <attrdef attribute="REF_SMALLINT"      maxtype="SMALLINT" remarks="-" title="Small Integer" />
      <attrdef attribute="REF_INTEGER"       maxtype="INTEGER"  remarks="-" title="Integer" />
      <attrdef attribute="REF_BIGINT"        maxtype="BIGINT"   remarks="-" title="Big Integer" />
      <attrdef attribute="REF_DECIMAL8x2"    maxtype="DECIMAL"  remarks="-" title="Decimal (6.2)" length="8" scale="2" />
      <attrdef attribute="REF_FLOAT"         maxtype="FLOAT"    remarks="-" title="Float" />
      <attrdef attribute="REF_AMOUNT"        maxtype="AMOUNT"   remarks="-" title="Amount" />
      <attrdef attribute="REF_DURATION"      maxtype="DURATION" remarks="-" title="Duration" />
<!--	GL -->
<!--	LONGALN -->
<!--	CRYPTO -->
<!--	CRYPTOX -->
<!--	BLOB -->
      <attrdef attribute="REF_CLOB" maxtype="CLOB" title="Clob" remarks="-" length="999999" restricted="true" />

      <!-- Attributes based on domains -->
      <attrdef attribute="REF_ALNDOMAINAB"      maxtype="UPPER"   domain="REFAB"         remarks="-" title="Upper domain with two items: A and B"            length="1" />
      <attrdef attribute="REF_ALNDOMAINAB_REQ"  maxtype="UPPER"   domain="REFAB"         remarks="-" title="Upper domain with two items: A and B (Required)" length="1" required="true" />
      <attrdef attribute="REF_ALNDOMAINAB_WITH_GETLIST"      maxtype="UPPER"   domain="REFAB"         remarks="-" title="Same domain as Upper domain with two items: A and B (fld class with a getList())"   classname="psdi.app.refapp.FldGetList"       length="1" />
      <attrdef attribute="REF_ALNDOMAINJK_WITH_GETLIST"   maxtype="UPPER" domain="REFJK" classname="psdi.app.refapp.FldGetList" remarks="-" title="Upper domain with two items: J and K (fld class defines a getList())" length="1" />
      <attrdef attribute="REF_ALNDOMAINXY_INHERIT_GETLST" maxtype="UPPER" domain="REFXY" classname="psdi.app.refapp.FldGetListNotInSubclass" remarks="-" title="Upper domain with two items: X and Y (fld class inherits getList())" length="1" />
      <attrdef attribute="REF_NUMDOMAIN100"     maxtype="INTEGER" domain="REF100"        remarks="-" title="Numeric domain with 100 numbers (0-99)"        length="2" />
      <attrdef attribute="REF_TABLEDOMAIN4"     maxtype="ALN"     domain="REFTABLE4"     remarks="-" title="Table domain with 4 entries"                   length="20" />
      <attrdef attribute="REF_TABLEDOMAINEMPTY" maxtype="ALN"     domain="REFTABLEEMPTY" remarks="-" title="Empty table domain"                            length="20" />
      <attrdef attribute="REF_UPPERDOMAINMATCH" maxtype="UPPER"   domain="REFMATCH"      remarks="-" title="Upper domain to check if X is correctly matched" length="3" />

      <attrdef attribute="REF_ALNDOMAINHEX"      maxtype="ALN"   domain="REFHEX"         remarks="-" title="Aln domain of Hex digits"            length="3" />
      <attrdef attribute="REF_TABLEDOMAINHEX"     maxtype="ALN"     domain="REFTABLEHEX"     remarks="-" title="Table domain of Hex and Decimal values"                   length="20" />

      <!-- Attributes that are made required by inputmode in the presentation -->
      <attrdef attribute="REF_ALNDOMAINAB_PRESREQ_RBG" maxtype="UPPER" domain="REFAB" remarks="-" title="Upper DomainAB Dynamic RadioButtonGroup Presentation Required" length="1" />
      <attrdef attribute="REF_ALN10_PRESREQ_RBG"       maxtype="ALN"                  remarks="-" title="ALN (10) Static RadioButtonGroup Presentation Required"        length="10" />
      
      <attrdef attribute="REF_ALNDOMAINAB_PRESREQ_COMBO" maxtype="UPPER" domain="REFAB" remarks="-" title="Upper DomainAB Combobox Presentation Required" length="1" />

      <attrdef attribute="REF_ALNDOMAINAB_PRESREQ_LISTBX" maxtype="ALN" domain="REFAB" remarks="-" title="Upper DomainAB Listbox Presentation Required" length="1" />

      <attrdef attribute="REF_ALN10_PRESREQ_TEXTBOX" maxtype="ALN" remarks="-" title="ALN (10) Textbox Presentation Required" defaultvalue="defaultval" length="10" />
      <attrdef attribute="REF_ALN10_PRESREQ_TEXTB_SIGOPT" maxtype="ALN" remarks="-" title="ALN (10) Textbox Presentation Required with a Sig Option to hide it" defaultvalue="defaultval" length="10" />
      
      <attrdef attribute="REF_ALN10_PRESREQ_MLTEXTBOX" maxtype="ALN" remarks="-" title="ALN (10) Multiline Textbox Presentation Required" defaultvalue="defaultval" length="10" />
      
      <attrdef attribute="REF_ALN10_PRESREQ_MPTXTBX1"     maxtype="ALN" remarks="-" title="ALN (10) Multipart Textbox Part 1 Presentation Required"          defaultvalue="defaultval" length="10" />
      <attrdef attribute="REF_ALN10_PRESREQ_MPTXTBX1_PWD" maxtype="ALN" remarks="-" title="ALN (10) Multipart Textbox Part 1 Password Presentation Required" defaultvalue="defaultval" length="10" />
      <attrdef attribute="REF_ALN10_PRESREQ_MPTXTBX2"     maxtype="ALN" remarks="-" title="ALN (10) Multipart Textbox Part 2 Presentation Required"          defaultvalue="defaultval" length="10" />
      <attrdef attribute="REF_ALN10_PRESREQ_MPTXTBX2_PWD" maxtype="ALN" remarks="-" title="ALN (10) Multipart Textbox Part 2 Password Presentation Required" defaultvalue="defaultval" length="10" />

      <!-- Attributes with validation -->
      <attrdef attribute="REF_ALN10_WSTARTWITHA"          maxtype="ALN"  length="10" classname="psdi.app.refapp.FldWarnDoesntStartWithA"                     remarks="-" title="ALN (10) Warning if doesn't start with 'A'" />
	  <attrdef attribute="REF_ALN10_ESTARTSWITHA"         maxtype="ALN"  length="10" classname="psdi.app.refapp.FldYNCErrorDoesntStartWithA"                 remarks="-" title="ALN (10) Yes No Cancel if doesn't start with 'A'" />
      <attrdef attribute="REF_ALN10_ESTARTSWITHA_WEMPOTH" maxtype="ALN"  length="10" classname="psdi.app.refapp.FldYNCErrorDoesntStartWithAWarnOnOtherEmpty" remarks="-" title="ALN (10) Yes No Cancel if doesn't start with 'A', warn if ALN10_EMPTY blank" />
      <attrdef attribute="REF_ALN10_EMPTY"                maxtype="ALN"  length="10"                                                                         remarks="-" title="ALN (10) Tied to ALN10_YESNOCANCEL_EMPTYWARNING" />

	  <attrdef attribute="REF_ALN10_PASSWORDLENGTH" maxtype="ALN" length="10" classname="psdi.app.refapp.FldPasswordLength" remarks="-" title="ALN (10) Warn if less than 6 characters" />
      
      <attrdef attribute="REF_YORN_WARNONYES"  maxtype="YORN" length="1" classname="psdi.app.refapp.FldYornWarnOnYes"  remarks="-" title="YorN, will get a warning if value is Y" defaultvalue="0" />     
      <attrdef attribute="REF_YORN_ERRORONYES" maxtype="YORN" length="1" classname="psdi.app.refapp.FldYornErrorOnYes" remarks="-" title="YorN, will get a error if value is Y"   defaultvalue="0" />     
      <attrdef attribute="REF_YORN_YNCONYES"   maxtype="YORN" length="1" classname="psdi.app.refapp.FldYornYNCOnYes"   remarks="-" title="YorN, Yes No Cancel if value is Y"      defaultvalue="0" />     

      <!-- Special HTML/XML characters -->
      <attrdef attribute="REF_SPECIALCHAR" maxtype="ALN" length="128" 
      			       title="&amp;&quot;'()%&lt;&gt;"
      			     remarks="&amp;&quot;'()%&lt;&gt;" 
      			defaultvalue="&amp;&quot;'()%&lt;&gt;" />
      <attrdef attribute="REF_ALNDOMAIN_SPECIALCHAR" maxtype="ALN" domain="REFSPECIALCHAR" length="1" remarks="-" title="ALN Domain html/xml special characters" />
      <attrdef attribute="REF_ENTITIES" maxtype="ALN" length="128" 
      			       title="&amp;amp;&amp;quot;&amp;#39;&amp;#40;&amp;#41;&amp;#37;&amp;lt;&amp;gt;"
      			     remarks="&amp;amp;&amp;quot;&amp;#39;&amp;#40;&amp;#41;&amp;#37;&amp;lt;&amp;gt;" 
      			defaultvalue="&amp;amp;&amp;quot;&amp;#39;&amp;#40;&amp;#41;&amp;#37;&amp;lt;&amp;gt;" />
      <attrdef attribute="REF_ALNDOMAIN_ENTITY" maxtype="ALN" domain="REFENTITY" length="10" remarks="-" title="ALN Domain html/xml entities" />

      <attrdef attribute="REF_SCRIPTINJECT" maxtype="ALN" length="256" 
      			       title="&gt;&lt;script&gt;alert('attr title');&lt;/script&gt;&lt;img src="
      			     remarks="&gt;&lt;script&gt;alert('attr remarks');&lt;/script&gt;&lt;img src=" 
      			defaultvalue="&gt;&lt;script&gt;alert('attr defval');&lt;/script&gt;&lt;img src=" />
