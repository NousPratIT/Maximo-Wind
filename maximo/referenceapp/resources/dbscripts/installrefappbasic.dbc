      <numericvalueinfo value="39" description="39" />
      <numericvalueinfo value="40" description="40" /><numericvalueinfo value="41" description="41" /><numericvalueinfo value="42" description="42" />
      <numericvalueinfo value="43" description="43" /><numericvalueinfo value="44" description="44" /><numericvalueinfo value="45" description="45" />
      <numericvalueinfo value="46" description="46" /><numericvalueinfo value="47" description="47" /><numericvalueinfo value="48" description="48" />
      <numericvalueinfo value="49" description="49" />
